   "source": [
    "def recursive_reverse(s):\n",
    "    if len(s) == 1:\n",
    "        return s\n",
    "    else: \n",
    "        return recursive_reverse(s[1:]) + s[0]\n",
    "    \n",
    "recursive_reverse(\"Rumman\")"
   ]
  }
